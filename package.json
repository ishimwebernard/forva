{
  "name": "elite-bn-backend",
  "version": "1.0.0",
  "description": "![CI/CD](https://github.com/atlp-rwanda/elite-bn-backend/workflows/CI/CD/badge.svg)  ![Code Climate maintainability](https://img.shields.io/codeclimate/maintainability/atlp-rwanda/elite-bn-backend)  [![Coverage Status](https://coveralls.io/repos/github/atlp-rwanda/elite-bn-backend/badge.svg?branch=ch-testing-setup)](https://coveralls.io/github/atlp-rwanda/elite-bn-backend?branch=ch-testing-setup)",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "test": "mocha --require @babel/register test/*.spec.js --timeout 500000 --exit || true",
    "build": "rm -rf ./build/ && babel ./src/ -d ./build/src/ && cp -r ./src/models/ ./build/models/ && babel ./src/index.js -d ./build/",
    "coverage": "nyc --reporter=html --reporter=text npm test",
    "coveralls": "npm run coverage && nyc report --reporter=lcov --reporter=text",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/elite-bn-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/elite-bn-backend/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/elite-bn-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "bcrypt": "^5.0.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "nodemailer": "^6.4.17",
    "nodemailer-express-handlebars": "^4.0.0",
    "nodemon": "^2.0.7",
    "npm": "^6.14.11",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
